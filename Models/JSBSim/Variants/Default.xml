      <diffuse>
       <red>0.034</red>
       <green>0.65</green>
       <blue>0.247</blue>
      </diffuse>
      <ambient>
       <red>0.034</red>
       <green>0.65</green>
       <blue>0.247</blue>
      </ambient>
      <transparency>
       <alpha>0.95</alpha>

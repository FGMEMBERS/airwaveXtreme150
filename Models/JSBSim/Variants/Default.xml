<?xml version="1.0"?>

<PropertyList>

 <fdm>
  <jsbsim>
   <aero>
    <max-glide-ratio>14.1</max-glide-ratio>
   </aero>
   <inertia>
    <pointmass-weight-lbs>200.</pointmass-weight-lbs>
   </inertia>
   <metrics>
    <Sw-sqft>154.0</Sw-sqft>
   </metrics>
  </jsbsim>
 </fdm>

 <sim>
 
  <model>

   <livery>
    <name type="string">Default</name>

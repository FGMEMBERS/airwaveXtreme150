       <red>0.034118</red>
       <green>0.65</green>
       <blue>0.246706</blue>
      </diffuse>
      <ambient>
       <red>0.034118</red>
       <green>0.65</green>
       <blue>0.246706</blue>
      </ambient>
      <transparency>
       <alpha>1.0</alpha>
      </transparency>
     </LeadingEdge>

     <SailUpper>
      <!-- rgb 0.949968 0.949968 0.949968 amb 0.949968 0.949968 0.949968 emis 0 0 0 spec 1 1 1 shi 32 trans 0.05  -->

       <red>0.949968</red>
       <green>0.949968</green>
       <blue>0.949968</blue>
      </diffuse>
      <ambient>
       <red>0.949968</red>
       <green>0.949968</green>
       <blue>0.949968</blue>
      </ambient>
      <transparency>
       <alpha>0.95</alpha>
      </transparency>
     </TrailingEdge>

     <SailLower>
      <!-- rgb 1 1 0 amb 1 1 0 emis 0 0 0 spec 1 1 1 shi 32 trans 0.05  -->
